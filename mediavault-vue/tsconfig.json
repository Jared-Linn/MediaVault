     {
       "compilerOptions": {
         "target": "esnext",
         "module": "esnext",
         "moduleResolution": "node",
         "strict": true,
         "jsx": "preserve",
         "importHelpers": true,
         "skipLibCheck": true,
         "esModuleInterop": true,
         "allowSyntheticDefaultImports": true,
         "sourceMap": true,
         "baseUrl": ".",
         "types": [
           "webpack-env",
           "vite/client"
         ],
         "paths": {
           "@/*": [
             "src/*"
           ]
         },
         "lib": [
           "esnext",
           "dom",
           "dom.iterable",
           "scripthost"
         ]
       },
       "include": [
         "src/**/*.ts",
         "src/**/*.d.ts",
         "src/**/*.tsx",
         "src/**/*.vue"
       ]
     }
